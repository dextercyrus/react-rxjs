{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://127.0.0.1:3000",
            "port": 9222,
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/src/index.js"
        },
        {
            "name": "attach",
            "type": "chrome",
            "request": "attach",
            "url": "http://127.0.0.1:3000",
            "port": 9222,
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/src/index.js"
        }
    ]
}